  "version": "0.2.0",
  "configurations": [{
    "type": "node",
    "request": "launch",
    "name": "Launch Program",
    "program": "${workspaceRoot}/dist/index.js",
    "cwd": "${workspaceRoot}"
  }, {
    "name": "Test Current File",
    "type": "node",
    "request": "launch",
    "program": "${workspaceRoot}/node_modules/.bin/_mocha",
    "stopOnEntry": false,
    "args": ["${file}", "--debug"],
    "cwd": "${workspaceRoot}",
    "preLaunchTask": null,
    "runtimeExecutable": null,
    "runtimeArgs": [
      "--nolazy"
    ],
    "env": {
      "NODE_ENV": "test"
    }
  }, {
    "type": "node",
    "request": "attach",
    "name": "Attach to Process",
    "port": 5858
  }]
}
